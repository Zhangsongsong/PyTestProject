<script type="text/javascript">
    var totalPageSize=2834;//总页码

    function goPage(pageNum){
        if(/^\d+$/.test(pageNum)==false) {
            return;
        }
        if(pageNum < 1) {
            pageNum = 1;
        }
        if(pageNum > totalPageSize) {
            if(totalPageSize>0){
                pageNum = totalPageSize;
            }else{
                pageNum=1;
            }
        }
        $("#pageCurrentPage").val(pageNum);
        window.location.href=$("#pageUrl").val()+'?'+$('#searchForm').serialize();
    }

    function showPageNumber() {
        var currentPage = 0<1?1:1;
        var totalPage = 2834;
        var pageHtml="";
        var maxNum_new = currentPage>4?7:8-currentPage;//最大显示页码数
        var discnt=1;
        for(var i=2; i>0; i--) {
            if(currentPage>i) {
                pageHtml = pageHtml + "<li><a href='javascript:goPage("+(currentPage-i)+")'>"+ (currentPage-i) +"</a></li>";
                discnt++;
            };
        }
        pageHtml = pageHtml + '<li ><a style="background-color: #ddd" href="javascript:void(0)">'+currentPage+'</a></li>';
        for(var i=1; i<maxNum_new; i++) {
            if(currentPage+i<=totalPage && discnt<5) {
                pageHtml = pageHtml + "<li><a href='javascript:goPage("+(currentPage+i)+")'>"+ (currentPage+i) +"</a></li>";
                discnt++;
            } else {
                break;
            };
        }
        $(pageHtml).insertBefore("#nextpage");
    }
    //跳转到页面
    function goPageByInput() {
        var pageNo = document.getElementById("pageNoIpt").value;
        if(/^\d+$/.test(pageNo)==false) {
            alert("只能输入整数，请重新输入！");
            document.getElementById("pageNoIpt").value='';
            return;
        }
        goPage(pageNo);
    };
    //显示中间部分页数
    showPageNumber();
</script></div>
