    $(function () {
        //懒加载
        $("img.lazy_img").lazyload({
            effect: "show"    // effect(特效),值有show(直接显示),fadeIn(淡入),slideDown(下拉)等,常用fadeIn

        });


        var e = $(".itop");
        $(window).scroll(function () {
            var t = $(document).scrollTop();
            if (t > 300) {
                e.fadeIn("slow")
            } else {
                e.fadeOut("slow")
            }
        });
        e.click(function () {
            $("html,body").animate({scrollTop: 0}, 500)
        })
    });
</script>
